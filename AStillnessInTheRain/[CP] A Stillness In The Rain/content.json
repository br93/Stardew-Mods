      "Default": true
    },
    "BooksellerSellsMusicSheet": {
      "AllowValues": "true, false",
      "Default": false
    },
    "TravellingCartSellsMusicSheet": {
      "AllowValues": "true, false",
      "Default": false
    },
    "SpawnMusicBenchAfterMastery": {
      "AllowValues": "true, false",
      "Default": true
    },
    "ChangeProjectile": {
      "AllowValues": "true, false",
      "Default": true
    }
  },
  "Changes": [
    {
      "LogName": "Include audio changes",
      "Action": "Include",
      "FromFile": "data/audio/audio.json, data/audio/audio-triggers.json"
    },
    {
      "LogName": "Include data changes",
      "Action": "Include",
      "FromFile": "data/animations.json, data/bundles.json, data/events.json, data/locations.json, data/mail.json, data/maps.json, data/shops.json, data/trinkets.json"
    },
    {
      "LogName": "Include object changes",
      "Action": "Include",
      "FromFile": "data/objects/harp.json, data/objects/music-bench.json, data/objects/music-sheet.json, data/objects/music-scroll.json"
    },
    {
      "LogName": "Include buff changes",
      "Action": "Include",
      "FromFile": "data/buffs/buffs-config.json, data/buffs/crops.json"
    }
  ]
}